	  console.log.apply(console, arguments);
	 }
	},
	test: function () {
	 if (this.level >= Logger.levels.TEST) {
	  console.log.apply(console, arguments);
	 }
	},
	error: function () {
	 if (this.level >= Logger.levels.ERROR) {
	  console.log.apply(console, arguments);
	 }
	}
   };
   
