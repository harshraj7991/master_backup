//folder App.js
import "./styles.css";
import Child from "./Child";

export default function App() {
  return (
    <div className="App">
      {/* <h1>Hello CodeSandbox</h1>
      <h2>Start editing to see some magic happen!</h2> */}
      <Child name="Kutchu" age="11" gender="Male" />
    </div>
  );
}

//folder child.js
import "./styles.css";
import Child2 from "./Child2";
import SoccerPlayer from "./SoccerPlayer";
import { useState } from "react";

export default function Child({ name, age, gender }) {
  // const players = ["Messi", "Ronaldo", "Mbappe"];
  const [count, setCount] = useState(10);
  function handleChange() {
    setCount(count + 1);
  }

  console.log(name);
  return (
    <>
      <Child2 count={count} />
      {/* <Child2 />
      <h1>Hello {name}</h1>
      {+age > 30 ? <h1>Age {age}</h1> : null}
      <h1>Gender {gender}</h1> */}
      <div>{count}</div>
      <button onClick={handleChange}> Increment Count </button>
      {/* {players.map((playerName, index) => (
        <SoccerPlayer key={index} name={playerName} idx={index} />
      ))} */}
    </>
  );
}


//folder child2.js

import "./styles.css";

export default function Child({ count }) {
  return (
    <div
      className="Child2"
      style={{
        fontSize: 24,
        margin: "0 auto",
        textAlign: "center",
        color: "Red"
      }}
    >
      <h1>Hello {count}</h1>
    </div>
  );
}

//folder soccerPlayer.js
import "./styles.css";

export default function SoccerPlayer({ name, idx }) {
  return (
    <div
      className="Player"
      style={{
        fontSize: 12,
        margin: "0 auto",
        textAlign: "center",
        color: "blue"
      }}
    >
      <h1>{name}</h1>
      <h1>{idx}</h1>
    </div>
  );
}

